{
  "name": "scoby/analytics",
  "description": "Client to measure web traffic in scoby",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Viergutz",
      "email": "peter@scoby.io"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Scoby\\": "src/scoby/"
    },
    "files": [
      "src/scoby/Analytics/Client.php",
      "src/scoby/Analytics/Laravel/Http/Middleware/CountPageView.php",
      "src/scoby/Analytics/Laravel/Providers/AnalyticsServiceProvider.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scoby\\Analytics\\Laravel\\Providers\\AnalyticsServiceProvider"
      ]
    }
  },
  "require": {
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "illuminate/support": "^9.25",
    "illuminate/http": "^9.25"
  }
}
